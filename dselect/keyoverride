32		Space
13		Return
27		Escape
28		^\
29		^]
30		^^
31		^_
34		Quote
39		Apostrophe
44		Comma
45		Hyphen
47		Slash
59		Semicolon
92		Backslash
96		Backquote
127		DEL
KEY_UP		Up
KEY_DOWN	Down
KEY_RIGHT	Right
KEY_LEFT	Left
KEY_IC		Insert
KEY_SIC		Shift Insert
KEY_DC		Delete
KEY_SDC		Shift Delete
KEY_NPAGE	Page Down
KEY_PPAGE	Page Up
KEY_CATAB	Clear Tabs
KEY_EIC		EIC
KEY_EOL		EOL
KEY_SEOL	Shift EOL
KEY_EOS		EOS
KEY_LL		Bottom
KEY_SF		Scroll Forward
KEY_SR		Scroll Reverse
KEY_SRESET	Soft Reset
KEY_SLEFT	Shift Left
KEY_SRIGHT	Shift Right
KEY_SPREVIOUS	Shift Previous
KEY_MAX		[elide]
KEY_MIN		[elide]

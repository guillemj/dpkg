#: deb-substvars.pod
#, fuzzy
#| msgid "B<vendor>"
msgid "B<vendor:Name>"
msgstr "B<vendor>"

#. type: textblock
#: deb-substvars.pod
msgid ""
"The current vendor name (since dpkg 1.20.0).  This value comes from the "
"B<Vendor> field for the current vendor's origin file, as B<dpkg-vendor>(1) "
"would retrieve it."
msgstr ""

#. type: =item
#: deb-substvars.pod
#, fuzzy
#| msgid "B<vendor>"
msgid "B<vendor:Id>"
msgstr "B<vendor>"

#. type: textblock
#: deb-substvars.pod
msgid ""
"The current vendor ID (since dpkg 1.20.0).  This is just the lowercase "
"variant of B<vendor:Name>."
msgstr ""


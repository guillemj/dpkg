#| " (tag1=i am marked|tag name with space)\"tagged quoted symbol\"@Base 1.0\n"
#| " (optional)tagged_unquoted_symbol@Base 1.0 1\n"
#| " untagged_symbol@Base 1.0\n"
msgid ""
"  (tag1=i am marked|tag name with space)\"tagged quoted symbol\"@Base 1.0\n"
"  (optional)tagged_unquoted_symbol@Base 1.0 1\n"
"  untagged_symbol@Base 1.0\n"
"\n"
msgstr ""

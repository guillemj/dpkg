#: dpkg.1:507
#, no-wrap
msgid "B<--root=>I<dir>"
msgstr "B<--root=>I<katalog>"

#. type: Plain text
#: dpkg.1:511
msgid ""
"Changing B<root> changes B<instdir> to I<dir> and B<admindir> to I<dir>B</"
"var/lib/dpkg>."
msgstr ""
"Zmiana B<root> zmienia B<instdir> na I<katalog> i B<admindir> na "
"I<katalog>B</var/lib/dpkg>."

#. type: TP

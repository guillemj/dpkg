msgid "B<-A>, B<--record-avail> I<package-file>..."
msgstr "B<-A>, B<--record-avail> I<plik_pakietu>..."

#
#. type: Plain text
#: dpkg.1:201
#, fuzzy
#| msgid ""
#| "Update B<dpkg> and B<dselect>'s idea of which packages are available with "

"arkitektur:"

#. type: verbatim
#: dpkg-architecture.pod
#, no-wrap
msgid ""
" dpkg-architecture -elinux-alpha\n"
"\n"
msgstr ""
"dpkg-architecture -elinux-alpha\n"
"\n"

#. type: verbatim
#: dpkg-architecture.pod
#, no-wrap
msgid ""
" dpkg-architecture -amips -elinux-mips\n"
"\n"
msgstr ""
"dpkg-architecture -amips -elinux-mips\n"
"\n"

#. type: textblock

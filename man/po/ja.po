#: dselect.pod
msgid "quit"
msgstr "終了 (quit)"

#. type: textblock
#: dselect.pod
#, fuzzy
#| msgid "Quit B<dselect>"
msgid "Quit B<dselect>."
msgstr "B<dselect> を終了する。"

#. type: textblock
#: dselect.pod
#, fuzzy
#| msgid "Exits the program with zero (successful) errorcode."

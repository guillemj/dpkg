# Global shell definitions for the autotest test suite

PATH="@abs_top_builddir@/src:$PATH"
export PATH

SED=@SED@

# Setup a sane environment

umask 0022

LC_ALL=C
export LC_ALL

TZ=UTC0
export TZ

SOURCE_DATE_EPOCH=0
export SOURCE_DATE_EPOCH

# Cleanup variables that might affect the tests.
unset GZIP
unset BZIP
unset XZ_OPT
unset XZ_DEFAULTS

